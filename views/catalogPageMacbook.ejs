			<form ng-submit="logIn()" style="margin-top: 30px;">
				<h3>Log In</h3>

				<div class="form-group">
					<input type="text"
					class="form-control"
					placeholder="Username"
					ng-model="user.username">
					</input>
				</div>

				<div class="form-group">
					<input type="password"
					class="form-control"
					placeholder="Password"
					ng-model="user.password">
					</input>
				</div>
