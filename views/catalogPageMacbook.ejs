		<script type="text/ng-template" id="/addProduct.html">
			<!-- Content -->
			<form ng-submit="createProduct()">
				<input type="text" ng-model="name" placeholder="Type Product Name"></input>
				<input type="text" ng-model="summary" placeholder="Type Summary"></input>
				<input type="text" ng-model="pathImgFull" placeholder="Type Path Img Full"></input>
				<input type="text" ng-model="pathImgThumb" placeholder="Type Path Img Thumb"></input>
				<input type="text" ng-model="price" placeholder="Type Product Price"></input>
				<textarea ng-model="description" placeholder="Type Description"></textarea>
				<button type="submit">Create New Product</button>
			</form>
		</script>

		<script type="text/ng-template" id="/register.html">
			<div class="page-header">
				<h1>Flapper News</h1>
			</div>

			<div ng-show="error" class="alert alert-danger row">
				<span>{{ error.message }}</span>
			</div>

			<form ng-submit="register()" style="margin-top: 30px;">
				<h3>Register</h3>

				<div class="form-group">
					<input type="text"
					class="form-control"
					placeholder="Username"
					ng-model="user.username">
					</input>
				</div>

				<div class="form-group">
					<input type="password"
					class="form-control"
					placeholder="Password"
					ng-model="user.password">
					</input>
				</div>
				<button type="submit" class="btn btn-primary">Register</button>
			</form>
		</script>

