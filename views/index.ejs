		<script type="text/ng-template" id="/catalogPageMacbook.html">
			<!-- Content -->
			<article id="content">
				<header>
					<h2>Macbook Accessories</h2>
					<p>{{summaryProductCatalog}}</p>
				</header>

				<div class="row">
					<div class="col-xs-6 col-md-3" ng-repeat="product in listMacProducts">
							<a href="#/product/{{$index}}" class="thumbnail">
							<img ng-src="/images/macbook/thumb/{{product.pathImgThumb}}" alt="..." />
						</a>
						<div style="text-align: center;">{{product.name}}</div>
						<div style="text-align: center;">&#36;{{product.price}}</div>
					</div>
				</div>
			</article>

		 <div class="col-md-2" ng-repeat="pic in pics">
